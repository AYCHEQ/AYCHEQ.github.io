bheader>\n                <v-container>\n                  <v-subheader class=\"headline\">{{day}}, {{date}}{{ord}} {{year}}</v-subheader>\n                  <v-spacer></v-spacer>\n\n                  <p class=\"text-xs-right\"><b>{{todos.length}}</b> Tasks</p>\n\n                  <v-flex xs12>\n                    <v-text-field clearable v-model=\"newTodo\" id=\"newTodo\" name=\"newTodo\" label=\"Type your task\" @keyup.enter=\"addTodo\">\n                    </v-text-field>\n                  </v-flex>\n                </v-container>\n                <div v-for=\"(todo, i) in todos\" v-bind:key=\"todo.id\">\n                  <v-list-tile avatar>\n                    <v-list-tile-action>\n                      <v-checkbox v-model=\"todo.done\"></v-checkbox>\n                    </v-list-tile-action>\n                    <v-list-tile-content>\n                      <v-list-tile-title :class=\"{\n                  done: todo.done\n                  }\" class=\"title\">{{todo.title | capitalize}}\n                      </v-list-tile-title>\n                      <v-list-tile-sub-title>Added on: {{date}}{{ord}} {{day}} {{year}}</v-list-tile-sub-title>\n                    </v-list-tile-content>\n                    <v-btn icon ripple color=\"red\"  v-if=\"todo.done\" @click=\"removeTodo(i)\">\n                      <v-icon class=\"white--text\">close</v-icon>\n                    </v-btn>\n                  </v-list-tile>\n                </div>\n              </v-list>\n            </v-card>\n            <p class=\"subheading\">This version (1.0) is in vuetify v1.5 new version coming with vuetify v2.0 with some features</p>\n  </v-app>\n</div>\n</v-flex>\n  </v-layout>\n      </v-container>\n    </v-content>\n</template>\n\n<script>\n\nexport default {\n  el: '#app',\n  data () {\n    return {\n      isDark: true,\n      show: true,\n      newTodo: '',\n      todo: [],\n      todos: [],\n      day: this.todoDay(),\n      date: new Date().getDate(),\n      ord: this.nth(new Date().getDate()),\n      year: new Date().getFullYear()\n    }\n  },\n  methods: {\n    addTodo () {\n      var value = this.newTodo && this.newTodo.trim()\n      if (!value) {\n        return\n      }\n      this.todos.push({\n        id: this.todos.length,\n        title: this.newTodo,\n        done: false\n      })\n      this.newTodo = ''\n    },\n    removeTodo (index) {\n      this.todos.splice(index, 1)\n    },\n    todoDay () {\n      var d = new Date()\n      var days = ['Sunday', 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday']\n      return days[d.getDay()]\n    },\n    nth (d) {\n      if (d > 3 && d < 21) return 'th'\n      switch (d % 10) {\n        case 1: return 'st'\n        case 2: return 'nd'\n        case 3: return 'rd'\n        default: return 'th'\n      }\n    }\n  },\n  filters: {\n    capitalize: function (value) {\n      if (!value) return ''\n      value = value.toString()\n      return value.charAt(0).toUpperCase() + value.slice(1)\n    }\n  }\n}\n</script>\n"]}]}