hover }\"\n          :open-delay=\"openDelay\"\n          :close-delay=\"closeDelay\"\n          :disabled=\"disabled\"\n          :value=\"value\"\n        >\n        <v-card\n        :elevation=\"hover ? 12 : 2\"\n          >\n          <v-card-text>\n            <v-container>\n              <h1> Register </h1>\n              <form @submit.prevent=\"onSignup\">\n                 <v-layout row>\n                  <v-flex xs12>\n                    <v-text-field\n                      name=\"firstname\"\n                      id=\"firstname\"\n                      :rules=\"nameRules\"\n                      :counter=\"20\"\n                      label=\"First name\"\n                    required></v-text-field>\n                  </v-flex>\n                </v-layout>\n                 <v-layout row>\n                  <v-flex xs12>\n                    <v-text-field\n                      v-model=\"lastname\"\n                      :rules=\"nameRules\"\n                      :counter=\"20\"\n                      label=\"Last name\"\n                    required></v-text-field>\n                  </v-flex>\n                </v-layout>\n                <v-layout row>\n                  <v-flex xs12>\n                    <v-text-field\n                      name=\"email\"\n                      label=\"Mail\"\n                      id=\"email\"\n                      :rules=\"emailRules\"\n                      v-model=\"email\"\n                      type=\"email\"\n                      required></v-text-field>\n                  </v-flex>\n                </v-layout>\n                <v-layout row>\n                  <v-flex xs12>\n                    <v-text-field\n            v-model=\"password\"\n            :append-icon=\"show1 ? 'mdi-eye' : 'mdi-eye-off'\"\n            :rules=\"[passwordRules.required, passwordRules.min]\"\n            :type=\"show1 ? 'text' : 'password'\"\n            name=\"password\"\n            label=\"Password\"\n            hint=\"At 